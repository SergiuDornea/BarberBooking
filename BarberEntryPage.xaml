<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BarberBooking.BarberEntryPage"
             Title="BarberEntryPage">
    <StackLayout>
        <Entry x:Name="numeEntry" Placeholder="Nume" />
        <Entry x:Name="prenumeEntry" Placeholder="Prenume" />
        <Entry x:Name="telefonEntry" Placeholder="Telefon" />

        <!-- Add other Entry fields for properties you want to edit -->

        <Button Text="Save" Clicked="OnSaveButtonClicked" />
    </StackLayout>
</ContentPage>